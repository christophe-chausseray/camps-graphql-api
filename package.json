{
  "name": "camps-graphql-api",
  "version": "1.0.0",
  "description": "GraphQL API to show campings location",
  "main": "./dist/index.ts",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "start": "concurrently \"npm:watch-ts\" \"npm:watch-server\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -W",
    "watch-server": "nodemon -w dist dist/index.js",
    "lint": "eslint . --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.4",
    "@types/express": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophe-chausseray/camps-graphql-api.git"
  },
  "keywords": [
    "GraphQL",
    "NodeJS",
    "Express",
    "Apollo",
    "Postgresql"
  ],
  "author": "Christophe Chausseray",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/christophe-chausseray/camps-graphql-api/issues"
  },
  "homepage": "https://github.com/christophe-chausseray/camps-graphql-api#readme"
}
